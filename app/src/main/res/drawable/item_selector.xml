<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- State for pressed state -->
    <item android:drawable="@drawable/item_rounded_pressed" android:state_pressed="true" />

    <!-- State for focused state -->
    <item android:drawable="@drawable/item_rounded_focused" android:state_focused="true" />

    <!-- Default state -->
    <item android:drawable="@drawable/item_rounded" />
</selector>
